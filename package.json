{
  "name": "new-template-front",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "prepare": "husky install",
    "_clean": "rimraf .next out build",
    "_control": "run-s lint format type-check test:unit && npm audit --production && echo \"\nControl finished!\n\"",
    "lint": "eslint --fix --ignore-path .eslintignore \"src/**/*.+(ts|tsx|js|jsx)\" && stylelint --fix --aei --config .stylelintrc \"src/**/*.+(css|scss|sass)\"",
    "format": "prettier --write --ignore-path .gitignore \"src/**/*.+(ts|tsx|js|jsx)\"",
    "type-check": "tsc",
    "test:unit": "cross-env JEST_HTML_REPORTER_OUTPUT_PATH=docs/tests/unit.html jest --no-cache --forceExit --config jest.config.js --detectOpenHandles ./tests/unit",
    "test:integration": "cross-env JEST_HTML_REPORTER_OUTPUT_PATH=docs/tests/integration.html jest --no-cache --forceExit --config jest.config.js --detectOpenHandles ./tests/integration",
    "dev": "next dev",
    "build-stats": "ANALYZE=true run-s build",
    "build": "run-s _clean && next build",
    "export": "npm run build && next export",
    "start": "next start"
  },
  "dependencies": {
    "@date-io/core": "1.3.13",
    "@date-io/date-fns": "1.3.13",
    "@material-ui/core": "4.11.4",
    "@material-ui/icons": "4.11.2",
    "@material-ui/lab": "4.0.0-alpha.58",
    "@material-ui/pickers": "3.3.10",
    "@reduxjs/toolkit": "1.6.0",
    "axios": "0.21.1",
    "babel-jest": "27.0.2",
    "date-fns": "2.22.1",
    "formik": "2.2.9",
    "i18next": "20.3.1",
    "lodash": "4.17.21",
    "next": "10.2.3",
    "next-seo": "4.24.0",
    "react": "17.0.2",
    "react-dom": "17.0.2",
    "react-redux": "7.2.4",
    "sweetalert2": "11.0.16"
  },
  "devDependencies": {
    "@next/bundle-analyzer": "10.2.3",
    "@testing-library/jest-dom": "5.13.0",
    "@testing-library/react": "11.2.7",
    "@types/jest": "26.0.23",
    "@types/lodash": "4.14.170",
    "@types/react": "17.0.9",
    "@types/testing-library__jest-dom": "5.9.5",
    "@typescript-eslint/eslint-plugin": "4.26.1",
    "@typescript-eslint/parser": "4.26.1",
    "cross-env": "7.0.3",
    "eslint": "7.28.0",
    "eslint-config-airbnb": "18.2.1",
    "eslint-config-prettier": "8.3.0",
    "eslint-import-resolver-typescript": "2.4.0",
    "eslint-plugin-import": "2.23.4",
    "eslint-plugin-jsx-a11y": "6.4.1",
    "eslint-plugin-prettier": "3.4.0",
    "eslint-plugin-react": "7.24.0",
    "eslint-plugin-react-hooks": "4.2.0",
    "husky": "6.0.0",
    "jest": "27.0.4",
    "jest-html-reporter": "3.4.1",
    "npm-run-all": "4.1.5",
    "prettier": "2.3.1",
    "rimraf": "3.0.2",
    "sass": "1.34.1",
    "stylelint": "13.13.1",
    "stylelint-config-recommended": "5.0.0",
    "stylelint-declaration-block-no-ignored-properties": "2.3.0",
    "stylelint-order": "4.1.0",
    "stylelint-scss": "3.19.0",
    "ts-jest": "27.0.3",
    "ts-node": "10.0.0",
    "typescript": "4.3.2"
  }
}